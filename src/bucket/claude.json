{
    "version": "0.4.9",
    "description": "Desktop application for Claude.ai",
    "homepage": "https://claude.ai/",
    "license": {
        "identifier": "Proprietary"
    },
    "architecture": {
        "64bit": {
            "url": "https://storage.googleapis.com/osprey-downloads-c02f6a0d-347c-492b-a752-3e0651722e97/nest-win-x64/AnthropicClaude-0.4.9-full.nupkg",
            "hash": "783f938bb5e4cd0a829133403f0e4cb8535d4d6d716667b886a291d62046049e",
            "installer": {
                "script": [
                    "$libDir = Join-Path $dir 'lib\\net45'",
                    "if (Test-Path $libDir) {",
                    "  Get-ChildItem $libDir | Move-Item -Destination $dir -Force",
                    "  Remove-Item (Join-Path $dir 'lib') -Recurse -Force -ErrorAction SilentlyContinue",
                    "  Remove-Item (Join-Path $dir '_rels') -Recurse -Force -ErrorAction SilentlyContinue",
                    "  Remove-Item (Join-Path $dir 'package') -Recurse -Force -ErrorAction SilentlyContinue",
                    "  Remove-Item (Join-Path $dir '[Content_Types].xml') -Force -ErrorAction SilentlyContinue",
                    "  Remove-Item (Join-Path $dir 'AnthropicClaude.nuspec') -Force -ErrorAction SilentlyContinue",
                    "}"
                ]
            }
        }
    },
    "shortcuts": [
        [
            "claude.exe",
            "Claude"
        ]
    ],
    "portableProfile": {
        "mappings": [
            {
                "label": "roaming",
                "source": "AppData/Claude",
                "target": "$persist_dir/roaming",
                "strategy": "symlink",
                "ensureTarget": true
            },
            {
                "label": "updater",
                "source": "LocalAppData/AnthropicClaude",
                "target": "$persist_dir/updater-blocker",
                "strategy": "symlink",
                "ensureTarget": true,
                "targetType": "file"
            }
        ],
        "options": {
            "log": true
        }
    },
    "checkver": {
        "url": "https://storage.googleapis.com/osprey-downloads-c02f6a0d-347c-492b-a752-3e0651722e97/nest-win-x64/RELEASES",
        "regex": "AnthropicClaude-([\\d.]+)-full\\.nupkg"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://storage.googleapis.com/osprey-downloads-c02f6a0d-347c-492b-a752-3e0651722e97/nest-win-x64/AnthropicClaude-$version-full.nupkg"
            }
        }
    }
}
